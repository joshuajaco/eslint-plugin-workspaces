{
  "name": "eslint-plugin-workspaces",
  "version": "0.9.0",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "monorepo",
    "packages",
    "workspaces",
    "yarn",
    "lerna",
    "npm",
    "pnpm",
    "bolt"
  ],
  "repository": "joshuajaco/eslint-plugin-workspaces",
  "license": "MIT",
  "exports": "./lib/index.js",
  "main": "lib/index.js",
  "files": [
    "lib/",
    "CHANGELOG.md"
  ],
  "scripts": {
    "format": "prettier -w .",
    "lint": "eslint . --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "test": "nyc -r=lcov -r=text mocha tests --recursive --file tests/setup.js"
  },
  "dependencies": {
    "find-workspaces": "^0.2.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "eslint": "^8.54.0",
    "eslint-plugin-eslint-plugin": "^5.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^3.1.0",
    "sinon": "^17.0.1"
  }
}
