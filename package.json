{
  "name": "eslint-plugin-workspaces",
  "version": "0.6.3",
  "main": "lib/index.js",
  "license": "MIT",
  "author": "Joshua Jacobowitz <joshua.jacobowitz@yahoo.de>",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "monorepo",
    "packages",
    "workspaces",
    "yarn",
    "lerna"
  ],
  "files": [
    "lib/",
    "CHANGELOG.md",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=8.10.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshuajaco/eslint-plugin-workspaces.git"
  },
  "prettier": "@joshuajaco/prettier-config",
  "scripts": {
    "format": "prettier --write \"**/*.{js,json,md,yml}\" --ignore-path .gitignore",
    "mocha": "mocha tests --recursive --file tests/setup.js",
    "test": "yarn lint && nyc yarn mocha",
    "coverage": "mkdir -p coverage && touch ./coverage/lcov.info && nyc report --reporter=text-lcov > ./coverage/lcov.info",
    "lint": "eslint . --ignore-path .gitignore",
    "lint:fix": "yarn lint --fix"
  },
  "dependencies": {
    "@joshuajaco/get-monorepo-packages": "^1.2.1"
  },
  "devDependencies": {
    "@joshuajaco/prettier-config": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-eslint-plugin": "^3.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.1.2",
    "mock-require": "^3.0.3",
    "nyc": "^15.1.0",
    "prettier": "^2.4.1"
  }
}
